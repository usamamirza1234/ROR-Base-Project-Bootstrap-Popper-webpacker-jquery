<p id="notice"><%= notice %></p>

<h1>App Functionalities</h1>

<table>
  <thead>
    <tr>
      <th>App version</th>
      <th>App package</th>
      <th>Is locked</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_functionalities.each do |app_functionality| %>
      <tr>
        <td><%= app_functionality.app_version %></td>
        <td><%= app_functionality.app_package %></td>
        <td><%= app_functionality.is_locked %></td>
        <td><%= link_to 'Show', app_functionality %></td>
        <td><%= link_to 'Edit', edit_app_functionality_path(app_functionality) %></td>
        <td><%= link_to 'Destroy', app_functionality, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Functionality', new_app_functionality_path %>
